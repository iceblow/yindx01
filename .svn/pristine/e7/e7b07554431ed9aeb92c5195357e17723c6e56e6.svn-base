   <!-- 查询当前小时时间段内的订单数量 -->
  <select id="selectOrderCount" resultType="java.lang.Long" >
    select count(orderid)
    from t_order
    where <![CDATA[ TIMESTAMPDIFF(HOUR,DATE_FORMAT(addtime,"%Y-%m-%d %h"),DATE_FORMAT(now(),"%Y-%m-%d %h")) = 0 ]]> 
  </select>
  
