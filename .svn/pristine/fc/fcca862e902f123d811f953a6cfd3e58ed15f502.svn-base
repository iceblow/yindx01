package com.uncleserver.common.push;

/**
 * 苹果原生推送信息Model
 * 
 * @author guok
 * @version 2017-01-23
 *
 */
public class IosPushDataBase {
	private String alert;// 通知消息内容
	private String sound;// 通知消息的声音,系统默认声音设置为"default"
	private int badge;// ICON上的角标

	public String getAlert() {
		return alert;
	}

	public void setAlert(String alert) {
		this.alert = alert;
	}

	public String getSound() {
		return sound;
	}

	public void setSound(String sound) {
		this.sound = sound;
	}

	public int getBadge() {
		return badge;
	}

	public void setBadge(int badge) {
		this.badge = badge;
	}
}
